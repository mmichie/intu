{
  "name": "simple-gpt4",
  "description": "A simple pipeline using GPT-4",
  "type": "simple",
  "version": "1.0",
  "provider": "openai",
  "provider_configs": {
    "openai": {
      "api_key": "${OPENAI_API_KEY}",
      "model": "gpt-4",
      "temperature": 0.7
    }
  },
  "options": {
    "retries": 3,
    "timeout": 30,
    "cache": 300
  }
}